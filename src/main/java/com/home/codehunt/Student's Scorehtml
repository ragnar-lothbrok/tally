                var timezone = jstz.determine_timezone(); // Now you have an instance of the TimeZone object.
                
                    var profile_timezone = "Asia/Kolkata";
                    if (timezone.name() !== profile_timezone) {
                        $.ajax({
                            type: "POST",
                            url:"/users/AJAX/set-timezone/",
                            data: {
                            'timezone': timezone.name(),
                            },
                        });
                    }
                

                
            });
